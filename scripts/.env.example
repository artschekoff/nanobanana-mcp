# Required
NANOBANANA_API_KEY=your_api_key_here

# API endpoints
NANOBANANA_BASE_URL=https://api.kie.ai/api/v1
NANOBANANA_CREATE_TASK_PATH=/jobs/createTask
NANOBANANA_GET_TASK_PATH=/jobs/recordInfo

# Model
NANOBANANA_MODEL=nano-banana-pro

# Timeouts and polling
NANOBANANA_TIMEOUT_SECONDS=90
NANOBANANA_POLL_INTERVAL_SECONDS=3
NANOBANANA_POLL_TIMEOUT_SECONDS=300

# Output and style source
NANOBANANA_OUTPUT_DIR=skills/nanobanana-imager/output
NANOBANANA_AGENTS_FILE=AGENTS.md
